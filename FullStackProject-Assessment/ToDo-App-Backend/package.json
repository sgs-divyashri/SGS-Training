{
  "name": "jwt-authentication",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npx ts-node src/server.ts",
    "db:seed": "cross-env NODE_OPTIONS=--require=ts-node/register sequelize-cli db:seed:all",
    "db:test:drop": "sequelize-cli db:drop --env test",
    "db:test:create": "sequelize-cli db:create --env test",
    "db:test:migrate": "cross-env TS_NODE_PROJECT=tsconfig.test.json NODE_OPTIONS=--require=ts-node/register sequelize-cli db:migrate --env test",
    "db:test:seed": "cross-env TS_NODE_PROJECT=tsconfig.test.json NODE_OPTIONS=--require=ts-node/register sequelize-cli db:seed:all --env test",
    "db:test:reset": "npm run db:test:drop && npm run db:test:create && npm run db:test:migrate && npm run db:test:seed",
    "test": "cross-env NODE_ENV=test lab -v --typescript test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@hapi/code": "^9.0.3",
    "@hapi/hapi": "^21.4.4",
    "@hapi/jwt": "^3.2.1",
    "@hapi/lab": "^26.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/hapi": "^18.0.15",
    "@types/node": "^24.10.1",
    "@types/uuid": "^10.0.0",
    "cross-env": "^10.1.0",
    "lab-transform-typescript": "^3.0.1",
    "sequelize-cli": "^6.6.5",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^17.3.1",
